function create_player()
	p={
		x=7,
		y=8,
		sprite=32,
		f=true,--add to
		hotdogs=0,
		items=0
	}
end

function player_movement()
	newx=p.x
	newy=p.y
	if (btnp(âž¡ï¸)) newx+=1 f=false
	if (btnp(â¬…ï¸)) newx-=1 f=true
	if (btnp(â¬‡ï¸)) newy+=1
	if (btnp(â¬†ï¸)) newy-=1
	
	interact(newx,newy)
	interact_item(newx,newy)
	interact_win(newx,newy)
	
	if not check_flag(0,newx,newy) then
		p.x=mid(0,newx,15)
		p.y=mid(0,newy,15)
	end
end

function interact(x,y)
	if check_flag(1,x,y) then
		pick_hotdog(x,y)
		create_prout(p.x*8,p.y*8)
	end
end

function draw_player()
	spr(p.sprite,p.x*8,p.y*8,1,1,f)
end
