compteurh=0

function spawn_hotdogs()

	while compteurh!=10 do
		
		hotdogx = flr(rnd(15))
		hotdogy = flr(rnd(15))


		new_hotdog={
			x=hotdogx*8,
			y=hotdogy*8
		}
	
--	create_message("x="..tostr(hotdogx))
	
		if check_flag(3,hotdogx,hotdogy) then
		mset(hotdogx,hotdogy,34)
		compteurh+=1
		end
	end
end

function next_tile(x,y)
	sprite=mget(x,y)
	mset(x,y,sprite+1)
end


function pick_hotdog(x,y)
	next_tile(x,y)
	p.hotdogs+=1
	sfx(0)
end
