function _init()
	create_player()
	prouts={}
	spawn_hotdogs()
	spawn_trap()
	init_text()
	state=3
	alreadyc=false
	ifpiece=false
end

function _update()
	if (state==0) update_game()
	if (state==1) update_gameover()
	if (state==2) update_win()
	if (state==3) update_logo()
	if (btnp(ðŸ…¾ï¸)) new_text({"text box","hello"})
end

function update_game()
	player_movement()
	interact()
	interact_item()
	interact_win()
	loose()
	update_prouts()
	count_time()
	coin_del()
end

function _draw()
	if (state==0) draw_game()
	if (state==1) draw_gameover()
	if (state==2) draw_win()
	if (state==3) draw_logo()
	if text.active==true then
		draw_text()
	end
end


function draw_game()
	cls()
	draw_map()
	draw_player()
	draw_prouts()
	draw_ui()
	draw_message()
	spawn_win()
end
